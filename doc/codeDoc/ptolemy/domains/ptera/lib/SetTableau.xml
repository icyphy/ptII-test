<?xml version="1.0" standalone="yes"?>
<!DOCTYPE doc PUBLIC "-//UC Berkeley//DTD DocML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/DocML_1.dtd">
<doc name="SetTableau" class="ptolemy.domains.ptera.lib.SetTableau">
  <description>
An event to set the state of a tableau.  </description>
  <author>Thomas Huining Feng</author>
  <version>$Id$</version>
  <since>Ptolemy II 8.0</since>
  <Pt.ProposedRating>Yellow (tfeng)</Pt.ProposedRating>
  <Pt.AcceptedRating>Red (tfeng)</Pt.AcceptedRating>
    <!--ptolemy.data.expr.Parameter-->
    <property name="alwaysOnTop">Whether the tableau should be always on top.</property>
    <!--ptolemy.data.expr.Parameter-->
    <property name="enabled">Whether controls in the tableau is enabled.</property>
    <!--ptolemy.data.expr.Parameter-->
    <property name="focused">Whether the tableau has the input focus.</property>
    <!--ptolemy.data.expr.StringParameter-->
    <property name="referredTableau">The tableau to be set. This must not be an empty string.</property>
    <!--ptolemy.data.expr.Parameter-->
    <property name="resizable">Whether the tableau is resizable.</property>
    <!--ptolemy.data.expr.Parameter-->
    <property name="screenLocation">Location of the tableau, or [-1, -1] if not changed.</property>
    <!--ptolemy.data.expr.Parameter-->
    <property name="screenSize">Size of the tableau, or [-1, -1] if not changed.</property>
    <!--ptolemy.data.expr.StringParameter-->
    <property name="title">The title of the tableau, or an empty string if not changed.</property>
    <!--ptolemy.data.expr.Parameter-->
    <property name="visible">Whether the tableau is visible.</property>
</doc>
